{
  "name": "Manhwa Reader",
  "description": "Fixes and hides ads for https://solomanhwa.com/manga/",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs", "activeTab", "scripting"],
  "host_permissions": ["https://solomanhwa.com/manga/*"]
}
